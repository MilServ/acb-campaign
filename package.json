{
  "name": "acb-campaign",
  "version": "1.0.0",
  "description": "Initializing repo for ACB's F2K campaign",
  "main": "server.js",
  "engine": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "start": "node server.js",
    "backend": "nodemon server.js",
    "frontend": "npm run front --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\"",
    "build": "cd client && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilServ/acb-campaign.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MilServ/acb-campaign/issues"
  },
  "homepage": "https://github.com/MilServ/acb-campaign#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "mongoose": "^5.9.27",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
